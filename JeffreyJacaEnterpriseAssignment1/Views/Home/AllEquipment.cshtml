@model IEnumerable<JeffreyJacaEnterpriseAssignment1.Models.Equipment>
@{
    Layout = "_BasicLayout";
    ViewBag.title = "All Equipment Listing";
}

        <div>
            <h2>Listing of All Equipment</h2>
            <table class="table-sm table-striped table-bordered">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Equipment Type</th>
                        <th>Description</th>
                        <th>Availability</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (JeffreyJacaEnterpriseAssignment1.Models.Equipment e in Model)
                    {
                    <tr>
                        <td>@e.Id</td>
                        <td>@e.Type</td>
                        <td>@e.Description</td>
                        <td>@e.Availability</td>
                    </tr>
                    }
                </tbody>
                
            </table>
        </div>
    